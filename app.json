{
    "expoDeploy": true,
    "name": "FosterPet",
    "displayName": "FosterPet",
    "expo": {
        "name": "FosterPet",
        "slug": "FosterPet",
        "version": "1.0.0",
        "privacy": "public",
        "sdkVersion": "41.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/expo/icon.png",
        "assetBundlePatterns": ["**/*"],
        "platforms": ["ios", "android", "web"],
        "splash": {
            "image": "./src/assets/expo/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "packagerOpts": {
            "config": "metro.config.js",
            "sourceExts": ["js", "jsx"]
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "ios": {
            "supportsTablet": true
        }
    }
}
